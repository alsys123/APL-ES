<iframe width="900" height="600" style="border:none;" srcdoc='
    <!DOCTYPE html>
    <html lang="en">
    <head>
      <meta charset="UTF-8">
      <title>Exam System</title>
        <link rel="stylesheet" href="https://alsys123.github.io/APL-ES/style.css?v=2">
    </head>
    <body>
      <div id="MainContent"></div>
      <script>
        fetch("https://alsys123.github.io/APL-ES/body.html")
          .then(response => response.text())
          .then(html => {
            document.getElementById("MainContent").innerHTML = html;
          });
      </script>
    </body>
    </html>
  '>
</iframe>
